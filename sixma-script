   _G.AutoFarmGunMastery = value
   if value == false then
      wait(0.2)
      topos(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
      game:GetService("Workspace").LOL:Destroy()
   end
end)

local KillAt = main:Slider("Kill At %",1,100,25,function(value)

    _G.Kill_At = value
end)

main:Toggle("Skill Z",true,function(value)
    _G.SkillZ = value
end)


